<!-- so101_with_objects.xml -->
<mujoco>
  <compiler meshdir="assets"/>
  <include file="so101_new_calib.xml"/>

  <visual>
    <headlight ambient="0.4 0.4 0.4" diffuse="0.8 0.8 0.8" specular="0.1 0.1 0.1"/>
  </visual>

  <asset>
      <texture type="skybox" builtin="gradient" rgb1="0.6 0.6 0.6" rgb2="0 0 0" width="512" height="512"/>
  </asset>

  <worldbody>
    <!-- Camera that will be randomized -->
    <camera name="random_camera" pos="0.7 0 0.5"/>

    <!-- Table -->
    <body name="table" pos="0 0 0">
        <!-- A large, thin box for the table surface -->
        <geom type="box" size="1.0 1.0 0.01" pos="0 0 -0.01" rgba="0.8 0.8 0.8 1" contype="1" conaffinity="1"/>
    </body>

    <body name="yellow_cube" pos="0.2 -0.2 0.025">
        <joint name="yellow_cube/freejoint" type="free" damping="0.01"/>
        <geom type="box" size="0.025 0.025 0.025" rgba="1 1 0 1" name="yellow_cube_geom" contype="1" conaffinity="1"/>
    </body>

    <body name="blue_tray" pos="0.2 0.2 0.01">
        <joint name="blue_tray/freejoint" type="free" damping="0.01"/>
        <geom type="box" size="0.1 0.15 0.01" rgba="0 0.5 1 1" name="blue_tray_geom" contype="1" conaffinity="1"/>
    </body>
  </worldbody>
</mujoco>
